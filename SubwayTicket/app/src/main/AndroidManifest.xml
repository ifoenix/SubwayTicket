<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.subwayticket">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.MainActivity"

             >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".view.LoginActivity"></activity>
        <activity android:name=".view.RegisterActivity"></activity>
        <activity android:name=".view.FindPasswordActivity"></activity>
        <activity android:name=".view.NewPasswordActivity"/>
        <activity android:name=".view.NoticeActivity"></activity>
        <activity android:name=".view.OperGuideActivity"></activity>
        <activity android:name=".view.OrderActivity"></activity>
        <activity android:name=".view.PersonalInfoActivity"></activity>
        <activity android:name=".view.SettingsActivity"></activity>
        <activity android:name=".view.SiteActivity"></activity>
        <activity android:name=".view.LineActivity"></activity>

    </application>

</manifest>